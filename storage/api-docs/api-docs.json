                    }
                }
            }
        },
        "/api/skills": {
            "get": {
                "tags": [
                    "Skills"
                ],
                "summary": "Get list of skills",
                "description": "Returns skills ordered by proficiency. When filtering by year, returns skills associated with experiences/education active in that year, plus personal skills (always shown).",
                "operationId": "getSkills",
                "parameters": [
                    {
                        "name": "year",
                        "in": "query",
                        "description": "Filter skills by year (based on experience and education relationships). Personal skills are always included.",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 2023
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "Laravel"
                                            },
                                            "category": {
                                                "type": "string",
                                                "example": "Backend"
                                            },
                                            "proficiency": {
                                                "type": "integer",
                                                "example": 95
                                            },
                                            "is_personal": {
                                                "description": "Indicates if this is a personal/self-taught skill",
                                                "type": "boolean",
                                                "example": false
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/hiking/route": {
            "get": {
                "tags": [
                    "Hiking"
                ],
                "summary": "Calculate a hiking route with 3D elevation profile",
                "description": "Calculates the route between two points using OSRM (foot profile) and enriches it with elevation data from local MDT files.",
                "operationId": "getHikingRoute",
                "parameters": [
                    {
                        "name": "start",
                        "in": "query",
                        "description": "Start coordinates (lat,lon)",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "27.9706,-15.6128"
                        }
                    },
                    {
                        "name": "end",
                        "in": "query",
                        "description": "End coordinates (lat,lon)",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "28.1235,-15.4363"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "type": {
                                            "type": "string",
                                            "example": "Feature"
                                        },
                                        "properties": {
                                            "properties": {
                                                "distance_km": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "elevation_gain_m": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "elevation_loss_m": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "max_elevation_m": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "min_elevation_m": {
                                                    "type": "number",
                                                    "format": "float"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "geometry": {
                                            "properties": {
                                                "type": {
                                                    "type": "string",
                                                    "example": "LineString"
                                                },
                                                "coordinates": {
                                                    "description": "Array of [lon, lat, ele] coordinates",
                                                    "type": "array",
                                                    "items": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input format"
                    },
                    "500": {
                        "description": "Server error or route calculation failed"
                    }
                }
            }
        }
    },
