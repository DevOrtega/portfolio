{
    "openapi": "3.0.0",
    "info": {
        "title": "Portfolio API",
        "description": "API documentation for the Portfolio project",
        "contact": {
            "email": "carlos@example.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/admin/auth/login": {
            "post": {
                "tags": [
                    "Admin Auth"
                ],
                "summary": "Login de administrador",
                "operationId": "a823865a41f39b60d928d4291f748602",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "admin@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login exitoso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Login successful"
                                        },
                                        "data": {
                                            "properties": {
                                                "user": {
                                                    "type": "object"
                                                },
                                                "token": {
                                                    "type": "string"
                                                },
                                                "expires_at": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Credenciales inválidas"
                    },
                    "429": {
                        "description": "Demasiados intentos"
                    }
                }
            }
        },
        "/api/admin/auth/logout": {
            "post": {
                "tags": [
                    "Admin Auth"
                ],
                "summary": "Logout de administrador",
                "operationId": "51ddc9a6f7b34d9351c3a8ac18f4e50b",
                "responses": {
                    "200": {
                        "description": "Logout exitoso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Logged out successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "No autenticado"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/admin/auth/me": {
            "get": {
                "tags": [
                    "Admin Auth"
                ],
                "summary": "Obtener usuario actual",
                "operationId": "ffc2a63891fcac2c33546ead07ea46a4",
                "responses": {
                    "200": {
                        "description": "Usuario actual",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "user": {
                                                    "type": "object"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "No autenticado"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/admin/auth/refresh": {
            "post": {
                "tags": [
                    "Admin Auth"
                ],
                "summary": "Refrescar token",
                "operationId": "3b3d5f0cb4d554530faeec7f5b43aebe",
                "responses": {
                    "200": {
                        "description": "Token refrescado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Token refreshed successfully"
                                        },
                                        "data": {
                                            "properties": {
                                                "token": {
                                                    "type": "string"
                                                },
                                                "expires_at": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "No autenticado"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/bus/data": {
            "get": {
                "tags": [
                    "Bus"
                ],
                "summary": "Get all bus data for the tracker demo",
                "description": "Returns companies, lines, routes, stops and configuration",
                "operationId": "getBusData",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "companies": {
                                            "type": "object"
                                        },
                                        "bus_lines": {
                                            "type": "object"
                                        },
                                        "main_lines": {
                                            "type": "object"
                                        },
                                        "routes": {
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            }
                                        },
                                        "stops": {
                                            "type": "object"
                                        },
                                        "map_config": {
                                            "type": "object"
                                        },
                                        "simulation_config": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/bus/routes": {
            "get": {
                "tags": [
                    "Bus"
                ],
                "summary": "Get all bus routes with coordinates",
                "operationId": "getBusRoutes",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
        "/api/bus/stops": {
            "get": {
                "tags": [
                    "Bus"
                ],
                "summary": "Get all bus stops",
                "operationId": "getBusStops",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
        "/api/bus/companies": {
            "get": {
                "tags": [
                    "Bus"
                ],
                "summary": "Get all bus companies",
                "operationId": "getBusCompanies",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
        "/api/bus/status": {
            "get": {
                "tags": [
                    "Bus"
                ],
                "summary": "Get OSRM and route status metrics",
                "description": "Returns statistics about route calculations, OSRM coverage, and system health",
                "operationId": "getBusRouteStatus",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "timestamp": {
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        "summary": {
                                            "properties": {
                                                "total_routes": {
                                                    "type": "integer"
                                                },
                                                "routes_with_osrm": {
                                                    "type": "integer"
                                                },
                                                "osrm_coverage": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "success_rate": {
                                                    "type": "number",
                                                    "format": "float"
                                                },
                                                "recent_failures": {
                                                    "type": "integer"
                                                },
                                                "recent_successes": {
                                                    "type": "integer"
                                                },
                                                "system_health": {
                                                    "type": "string",
                                                    "enum": [
                                                        "excellent",
                                                        "good",
                                                        "warning",
                                                        "critical"
                                                    ]
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "route_quality": {
                                            "properties": {
                                                "excellent": {
                                                    "type": "integer"
                                                },
                                                "good": {
                                                    "type": "integer"
                                                },
                                                "fair": {
                                                    "type": "integer"
                                                },
                                                "poor": {
                                                    "type": "integer"
                                                },
                                                "unknown": {
                                                    "type": "integer"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "alerts": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "type": {
                                                        "type": "string"
                                                    },
                                                    "message": {
                                                        "type": "string"
                                                    },
                                                    "threshold": {
                                                        "type": "number"
                                                    },
                                                    "current": {
                                                        "type": "number"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/contact": {
            "post": {
                "tags": [
                    "Contact"
                ],
                "summary": "Send contact form message",
                "description": "Handle contact form submission.",
                "operationId": "9ae6c90eb61192febf25c5af341ce70d",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "subject",
                                    "message"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "john@example.com"
                                    },
                                    "subject": {
                                        "type": "string",
                                        "example": "Project Inquiry"
                                    },
                                    "message": {
                                        "type": "string",
                                        "example": "Hello, I would like to discuss a project..."
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Message sent successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Mensaje enviado correctamente"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error"
                    },
                    "429": {
                        "description": "Too many requests"
                    }
                }
            }
        },
        "/api/education": {
            "get": {
                "tags": [
                    "Education"
                ],
                "summary": "Get list of education",
                "operationId": "getEducation",
                "parameters": [
                    {
                        "name": "year",
                        "in": "query",
                        "description": "Filter education by year",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
