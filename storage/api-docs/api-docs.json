    "paths": {
        "/api/education": {
            "get": {
                "tags": [
                    "Education"
                ],
                "summary": "Get list of education",
                "operationId": "getEducation",
                "parameters": [
                    {
                        "name": "year",
                        "in": "query",
                        "description": "Filter education by year",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
